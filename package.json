{
  "name": "hexo-renderer-art",
  "version": "0.1.0",
  "description": "Add art-template support for hexo",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "keywords": [
    "hexo",
    "art-template",
    "renderer"
  ],
  "scripts": {
    "test": "mocha test/index.js",
    "eslint": "eslint lib/*.js",
    "test-cov": "NODE_ENV=test YOURPACKAGE_COVERAGE=1 ./node_modules/.bin/mocha --require blanket  ./test/*.js | ./node_modules/coveralls/bin/coveralls.js"
  },
  "author": "jeffwang <cunxuanwang@163.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    
  },
  "devDependencies": {
    "blanket": "^1.2.3",
    "chai": "^4.0.2",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^2.13.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-html": "^3.0.0",
    "eslint-plugin-import": "^2.7.0",
    "mocha": "^3.4.2",
    "mocha-lcov-reporter": "^1.3.0"
  },
  "dependencies": {
    "art-template": "^4.12.1"
  },
  "config": {
    "blanket": {
      "pattern": [ "" ],
      "data-cover-never": [ "node_modules", "test" ]
    }
  }
}
